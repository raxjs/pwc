<script>
import { compile, reactive  } from 'pwc';

export default class RuntimeComponent extends HTMLElement {
  title = 'pwc';

  handleClick() {
    this.title = 'changed title';
  }

  get template() {
    return compile(`<div class="container">{{ ${this.title} }}</div>`);
  }
}
</script>

<style>
  .container {
    color: blue;
  }
</style>
